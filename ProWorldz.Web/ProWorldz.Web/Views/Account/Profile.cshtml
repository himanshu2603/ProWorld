@model ProWorldz.BL.BusinessModel.UserBM
@{
    ViewBag.Title = "Home Page";
    
}


<body class="login">


    <div id="content">
        <div class="container-fluid">
            <div class="lock-container">
                <h1>Edit Profile</h1>
                <div class="panel panel-default text-center">

                    @if (Model != null)
                    {
                        <h2>@Model.Name</h2>
                    }

                </div>
            </div>
        </div>
    </div>

     <br />
    <br />
    <br />   <br />
    <br />
    <br />   <br />
    <br />
    <br />   <br />
    <br />
    <div class="container">
        <div class="tabbable tabs-vertical tabs-left">
            <!-- Tabs -->
            <ul class="nav nav-tabs" tabindex="6" style="overflow: hidden; outline: none;">
                <li class="active"><a href="#home4" data-toggle="tab"><i class="fa fa-fw fa-home"></i>Home</a>
                </li>
                <li><a href="#profile4" data-toggle="tab"><i class="fa fa-fw fa-user"></i>Profile</a>
                </li>
                <li><a href="#messages4" data-toggle="tab"><i class="fa fa-fw fa-envelope"></i>Messages</a>
                </li>
            </ul>
